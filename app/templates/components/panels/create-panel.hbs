<center>
  <h3>Create New {{display-module-name}}</h3>
  {{#each metaData as |key|}}
    {{#if (eq key.name "panel_header")}}
      {{#each key.fields as |field|}}
        <br />
        <label>{{field.name}}: </label>
        <br />
        {{input type="text" id = field.name }}
        <br />
        <br />
      {{/each}}
      <button class="btn btn-primary">Submit</button>
    {{/if}}
  {{/each}}
</center>
{{yield}}
